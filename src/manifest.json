{
    "name": "Blocker",
    "description": "Block it!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Some Event",
        "default_popup": "popup.html"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "host_permissions": ["http://*/*","https://*/*"]
}